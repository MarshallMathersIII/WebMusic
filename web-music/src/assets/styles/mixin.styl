// 不换行
no-wrap()
  text-overflow ellipsis
  overflow hidden
  white-space nowrap
// 按钮样式
btn()
  margin 20px 10px
  height 40px
  background-color red
  color #ffffff
  line-height 40px
  text-align center
  border-radius 5px
  // 按钮不可点击样式
btn-inactive()
  margin 20px 10px
  height 40px
  background-color red
  color #ffffff
  line-height 40px
  text-align center
  border-radius 5px
  opacity 0.5
// 分割线
divider()
  height 4px
  background-color #F7F8FA
divider-1()
  height 1px
  background-color #F7F8FA
